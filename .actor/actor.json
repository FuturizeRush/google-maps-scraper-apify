{
    "actorSpecification": 1,
    "name": "google-maps-business-scraper",
    "title": "Google Maps Business Scraper",
    "description": "Extract business listings from Google Maps with complete information including addresses, phone numbers, operating hours, emails, and more. Get up to 120 results per search with support for 8 languages and detailed business data extraction.",
    "version": "0.5",
    "meta": {
        "templateId": "js-start"
    },
    "input": "./INPUT_SCHEMA.json",
    "dockerfile": "./Dockerfile",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Google Maps businesses",
            "views": {
                "businesses": {
                    "title": "Businesses",
                    "transformation": {
                        "fields": [
                            "name",
                            "placeId",
                            "rating",
                            "reviews",
                            "address",
                            "businessType",
                            "url",
                            "phone",
                            "website"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "name": {
                                "label": "Business Name"
                            },
                            "rating": {
                                "label": "Rating",
                                "format": "number"
                            },
                            "reviews": {
                                "label": "Reviews Count",
                                "format": "number"
                            }
                        }
                    }
                }
            }
        }
    }
}